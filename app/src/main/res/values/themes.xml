<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Wingu" parent="Theme.MaterialComponents.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grey_50_light</item>
        <item name="colorPrimaryVariant">@color/grey_50_dark</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/grey_800</item>
        <item name="colorSecondaryVariant">@color/grey_50_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSurface</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>
